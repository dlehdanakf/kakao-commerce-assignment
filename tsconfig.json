{
    "compilerOptions": {
        "baseUrl": ".",
        "target": "es5",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "allowJs": true,
        "target": "ES2015",
        "moduleResolution": "node",
        "lib": [
            "es6",
            "DOM"
        ]
    },
    "exclude": [
        "**/*.spec.ts",
        "**/*.test.ts"
    ]
}
